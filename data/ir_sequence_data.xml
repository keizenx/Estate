<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Séquence pour les locations -->
        <record id="seq_estate_location" model="ir.sequence">
            <field name="name">Séquence de location</field>
            <field name="code">estate.location</field>
            <field name="prefix">LOC/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les factures -->
        <record id="seq_estate_facture" model="ir.sequence">
            <field name="name">Séquence de facture</field>
            <field name="code">estate.facture</field>
            <field name="prefix">FACT/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Séquence pour les états des lieux -->
        <record id="seq_estate_etat_des_lieux" model="ir.sequence">
            <field name="name">Séquence d'état des lieux</field>
            <field name="code">estate.etat.des.lieux</field>
            <field name="prefix">EDL/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Séquence pour les rappels -->
        <record id="seq_estate_rappel" model="ir.sequence">
            <field name="name">Séquence de rappel</field>
            <field name="code">estate.rappel</field>
            <field name="prefix">RAP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo> 